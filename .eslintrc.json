{
  "extends": "airbnb",
  "rules": {
    "no-console": "off",
    "func-names": [
      "error",
      "as-needed"
    ],
    "consistent-return": "off",
    "no-useless-escape": "off",
    "arrow-body-style": [
      "error",
      "as-needed"
    ],
    "curly": [
      "error",
      "multi"
    ],
    "react/jsx-filename-extension": [
      1,
      {
        "extensions": [
          ".js",
          ".jsx"
        ]
      }
    ],
    "no-alert": "off",
    "no-confusing-arrow": "off",
    "no-unused-vars": [
      1,
      {
        "argsIgnorePattern": "^_"
      }
    ]
  },
  "globals": {
    "localStorage": false,
    "alert": false,
    "fetch": false
  }
}